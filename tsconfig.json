{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "noImplicitAny": false,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "module": "commonjs",
    "target": "es6",
    "lib": ["es2015", "es2017"]
  }
}


